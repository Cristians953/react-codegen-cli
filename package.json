{
  "name": "react-codegen-cli",
  "version": "1.0.3",
  "description": "Software Development Kit for React",
  "main": "lib/index.js",
  "bin": {
    "react-codegen": "lib/bin.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "prepublish": "yarn build && yarn clean",
    "postpublish": "yarn build",
    "build": "babel src --out-dir lib --copy-files --extensions \".ts\"",
    "watch": "yarn build --watch --source-maps inline",
    "lint": "eslint src --ext .ts",
    "start": "node ./lib/bin.js",
    "gen": "node ./lib/__tests__/script.js",
    "clean": "rm -rf ./src/__tests__/generated/*; rm -rf ./lib/__tests__"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "dependencies": {
    "@babel/generator": "^7.11.0",
    "@babel/parser": "^7.11.3",
    "@babel/template": "^7.10.4",
    "@babel/traverse": "^7.11.0",
    "@babel/types": "^7.11.0",
    "chalk": "^4.1.0",
    "console-clear": "^1.1.1",
    "cosmiconfig": "^6.0.0",
    "detect-indent": "^6.0.0",
    "find-up": "^4.1.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "lodash": "^4.17.19",
    "log-symbols": "^4.0.0",
    "prettier": "^2.0.5",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/babel-generator": "^6.25.3",
    "@types/babel-traverse": "^6.25.5",
    "@types/babel-types": "^7.0.8",
    "@types/babel__generator": "^7.6.1",
    "@types/fs-extra": "^9.0.1",
    "@types/inquirer": "^6.5.0",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "typescript": "^3.9.7"
  },
  "keywords": [
    "react",
    "React CLI",
    "React CodeGen",
    "devkit",
    "sdk",
    "React DevKit",
    "code generator"
  ],
  "repository": "https://github.com/Cristians953/react-codegen-cli.git",
  "author": "Cristian Vasiliev <vasiliev.cristian99@gmail.com>",
  "license": "MIT"
}
