{
  "name": "react-codegen-cli",
  "version": "1.1.0",
  "description": "Software Development Kit for React",
  "main": "lib/index.js",
  "bin": {
    "react-codegen": "lib/bin.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "commit": "cz",
    "test": "nyc ts-mocha --config .mocharc.js --paths -p ./tsconfig.json ./src/tests/unit/**/*.test.ts",
    "prepublish": "yarn build",
    "postpublish": "yarn build",
    "build": "babel src --out-dir lib --copy-files --extensions \".ts\"",
    "watch": "yarn build --watch --source-maps inline",
    "lint": "eslint src --ext .ts",
    "start": "node ./lib/bin.js",
    "release": "semantic-release",
    "semantic-release": "semantic-release"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "src/tests"
    ],
    "include": [
      "src"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "dependencies": {
    "@babel/generator": "^7.11.0",
    "@babel/parser": "^7.11.3",
    "@babel/template": "^7.10.4",
    "@babel/traverse": "^7.11.0",
    "@babel/types": "^7.11.0",
    "@types/sinon": "^9.0.10",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "concat-stream": "^2.0.0",
    "console-clear": "^1.1.1",
    "cosmiconfig": "^6.0.0",
    "detect-indent": "^6.0.0",
    "find-up": "^4.1.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "lodash": "^4.17.19",
    "log-symbols": "^4.0.0",
    "prettier": "^2.0.5",
    "sinon": "^9.2.3",
    "tsconfig-paths": "^3.9.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/babel-generator": "^6.25.3",
    "@types/babel-traverse": "^6.25.5",
    "@types/babel-types": "^7.0.8",
    "@types/babel__generator": "^7.6.1",
    "@types/chai": "^4.2.14",
    "@types/concat-stream": "^1.6.0",
    "@types/expect": "^24.3.0",
    "@types/fs-extra": "^9.0.1",
    "@types/inquirer": "^6.5.0",
    "@types/mocha": "^8.2.0",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "execa": "^5.0.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "semantic-release": "^17.3.4",
    "ts-mocha": "^8.0.0",
    "typescript": "^3.9.7"
  },
  "keywords": [
    "react",
    "React CLI",
    "React CodeGen",
    "devkit",
    "sdk",
    "React DevKit",
    "code generator"
  ],
  "repository": "https://github.com/Cristians953/react-codegen-cli.git",
  "author": "Cristian Vasiliev <vasiliev.cristian99@gmail.com>",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
