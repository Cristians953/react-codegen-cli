{
  "presets": [
    [
      "@babel/preset-env",
      {
        "exclude": ["transform-regenerator"]
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["module-resolver", {
      "root": ".",
      "alias": {
        "@": "./src"
      }
    }]
  ]
}
